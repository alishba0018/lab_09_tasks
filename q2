#include <stdio.h>
void swap(int a, int b){
    int temp;
    temp=a;
    a=b;
    b=temp;
    printf("reversed first number is:%d \n",a);
    printf("reversed second number is:%d",b);
    return;
}
int main(){
    int a,b;
    printf("enter first number:");
    scanf("%d",&a);
    printf("enter second number:");
    scanf("%d",&b);
    swap(a,b);
    return 0;
}
